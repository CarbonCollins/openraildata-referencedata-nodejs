{
  "name": "@openrailuk/referencedata",
  "version": "2.0.0",
  "displayName": "Open Rail UK Reference Data",
  "description": "A package for accessing the the UK National Rails reference data FTP server as well as some helper functions to process and use the reference data",
  "license": "MIT",
  "main": "index",
  "module": "index.mjs",
  "homepage": "https://gitlab.com/openrail/uk/referencedata-nodejs/uploads/d88da25cc746937bb208f9d3e13ed90c/reference.svg",
  "keywords": [
    "openrail",
    "openrailuk",
    "openraildata",
    "national",
    "network",
    "rail",
    "nationalrail",
    "networkrail",
    "api",
    "reference"
  ],
  "author": {
    "name": "Steven Collins",
    "email": "steven@carboncollins.uk",
    "url": "https://carboncollins.uk"
  },
  "scripts": {
    "prepublishOnly": "gulp compile",
    "publish-npm": "npm publish --scope=@openrailuk --access=public",
    "test": "mocha ./tests/test.all.js",
    "coverage": "./node_modules/.bin/nyc -x \"**/lib/es6/**/*.js\" -x \"**/src/**/*.js\" -x \"**/tests/**/*.js\" --reporter=\"text-summary\" --reporter=\"lcov\" yarn test --reporter=\"mochawesome\"",
    "compile": "gulp compile",
    "compileES5": "gulp compileES5",
    "compileES6": "gulp compileES6",
    "compileQuality": "gulp prepCodeQuality",
    "build": "",
    "generateDocs": "gulp generateDocs"
  },
  "repository": {
    "type": "gitlab",
    "url": "https://gitlab.com/openrail/uk/referencedata-nodejs"
  },
  "bugs": {
    "url": "https://gitlab.com/openrail/uk/referencedata-nodejs/issues"
  },
  "badges": [
    "https://img.shields.io/badge/license-MIT-blue.svg",
    "https://img.shields.io/badge/country-UK-blue.svg",
    "https://img.shields.io/badge/status-WIP-yellow.svg",
    "https://gitlab.com/openrail/uk/referencedata-nodejs/badges/master/pipeline.svg",
    "https://gitlab.com/openrail/uk/reference-nodejs/badges/master/coverage.svg",
    "https://img.shields.io/npm/v/@openrailuk/referencedata.svg"
  ],
  "markdown": "standard",
  "publisher": "npm",
  "engines": {
    "node": ">=6.x",
    "yarn": ">=1.x"
  },
  "readmeFilename": "README.md",
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "doc": "./docs",
    "lib": "./lib",
    "test": "./tests"
  },
  "dependencies": {
    "@openrailuk/common": "^3.2.0",
    "fs-extra": "^6.0.1",
    "ftp": "^0.3.10",
    "moment": "^2.22.2",
    "progress": "^2.0.0",
    "progress-stream": "^2.0.0",
    "uuid": "^3.2.1",
    "xml2js": "^0.4.19",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-rename-import": "^2.2.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-uuid": "^1.0.6",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-mocha": "^5.0.0",
    "ftpd": "^0.2.15",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-clean": "^0.4.0",
    "gulp-rename": "^1.3.0",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.2",
    "nyc": "^12.0.2"
  }
}
